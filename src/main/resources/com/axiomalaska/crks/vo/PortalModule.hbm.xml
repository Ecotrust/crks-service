<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.axiomalaska.crks.vo.PortalModule" table="portal_module" schema="crks">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="sequence">
                <param name="sequence">portal_module_id_seq</param>
            </generator>
        </id>
        <many-to-one name="module" class="com.axiomalaska.crks.vo.Module" fetch="select">
            <column name="module_id" not-null="true" />
        </many-to-one>
        <many-to-one name="portal" class="com.axiomalaska.crks.vo.Portal" fetch="select">
            <column name="portal_id" not-null="true" />
        </many-to-one>
        <property name="sortOrder" type="int">
            <column name="sort_order" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
