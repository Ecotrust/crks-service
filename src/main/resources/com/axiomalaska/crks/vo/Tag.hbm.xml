<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.axiomalaska.crks.vo.Tag" table="tag" schema="crks">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="sequence">
                <param name="sequence">tag_id_seq</param>
            </generator>
        </id>
        <property name="label" type="string">
            <column name="label" length="256" not-null="true" />
        </property>
        <property name="tagTypeId" type="int">
            <column name="tag_type_id" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
